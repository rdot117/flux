{
    "name": "flux",
    "emitLegacyScripts": false,

    "tree": {
        "$className": "DataModel",

        "ReplicatedStorage": {
            "packages": {
                "$path": "build/packages"
            },

            "shared": {
                "$path": "build/src/shared"
            },

            "client": {
                "$path": "build/src/client"
            }
        },

        "ServerScriptService": {
            "server": {
                "$path": "build/src/server"
            }
        },

        "Workspace": {
            "$properties": {
                "FilteringEnabled": true
            }
        },

        "SoundService": {
            "$properties": {
                "RespectFilteringEnabled": true
            }
        }
    }
}
