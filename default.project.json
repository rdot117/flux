{
    "name": "luau-crate",
    "emitLegacyScripts": false,

    "tree": {
        "$className": "DataModel",

        "ReplicatedStorage": {
            "packages": {
                "$path": "packages"
            },

            "tests": {
                "$path": "tests"
            }
        },

        "Workspace": {
            "$properties": {
                "FilteringEnabled": true
            }
        },

        "SoundService": {
            "$properties": {
                "RespectFilteringEnabled": true
            }
        }
    }
}
